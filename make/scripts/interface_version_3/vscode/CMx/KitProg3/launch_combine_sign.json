            "numberOfProcessors": &&_MTB_RECIPE__PROCESSOR_COUNT&&,
            "targetProcessor": &&_MTB_RECIPE__TARGET_PROCESSOR_NUMBER&&,  // Set to 0 for the CM0+, set to 1 or 2 for the CM7
            "postStartSessionCommands": [       
